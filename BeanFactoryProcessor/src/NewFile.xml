<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>



<bean id="PointA" class="Point"  >
<property name ="x" value="1"/>
<property name ="y" value="4"/>
</bean>

<bean id="PointB" class="Point"  >
<property name ="x" value="11"/>
<property name ="y" value="42"/>
</bean>

<bean id="PointC" class="Point"  >
<property name ="x" value="111"/>
<property name ="y" value="412"/>
</bean>

<bean id="triangle1" class="Triangle"  >
<property name="PointA" ref="PointA"/>
<property name="PointB" ref="PointB"/>
<property name="PointC" ref="PointC"/>

</bean>

<bean class="DisplayName"/>

</beans>
